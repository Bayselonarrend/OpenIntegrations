def test_tools(){
					

               catchError(buildResult: 'FAILURE', stageResult: 'FAILURE') {   
					powershell encoding: 'UTF-8', script:'./oint.exe tools ParseJSON --json "test" --nesting "test" --name "test" --enc "test" --debug --test '
               }

               catchError(buildResult: 'FAILURE', stageResult: 'FAILURE') {   
					powershell encoding: 'UTF-8', script:'./oint.exe tools ConvertBase64ToBinary --in "test" --debug --test '
               }

}


