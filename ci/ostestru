               powershell encoding: 'UTF-8', script:'"C:\Program Files (x86)\GnuPG\bin\gpg.exe" --quiet --batch --yes --decrypt --passphrase="${GPGKEY}" --output ./data.json ./data.json.gpg'
            }
            
