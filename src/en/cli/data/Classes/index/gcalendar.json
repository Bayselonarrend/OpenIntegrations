					"name": "--calendar",
					"types": "String",
					"description": "Calendar ID",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--title",
					"types": "String",
					"description": "New name",
					"default": "Empty value",
					"short": "-l"
				},
				{
					"name": "--description",
					"types": "String",
					"description": "New calendar description",
