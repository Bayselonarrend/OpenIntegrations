			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--title",
					"types": "String",
					"description": "Name of the created calendar",
					"default": null,
					"short": "-l"
				}
			],
			"region": "Calendar metadata management"
		},
		{
