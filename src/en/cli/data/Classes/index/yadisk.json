{
	"module": "OPI_YandexDisk",
	"library": "yadisk",
	"self_depend": false,
	"regions": [
		{
			"name": "Authorization",
			"methods": [
				"GetConfirmationCode",
				"ConvertCodeToToken",
				"RefreshToken"
			]
		},
		{
			"name": "File and folder management",
			"methods": [
				"GetDiskInformation",
				"CreateFolder",
				"GetObject",
				"DeleteObject",
				"CreateObjectCopy",
				"GetDownloadLink",
				"GetFileUploadLink",
				"DownloadFile",
				"GetFilesList",
				"MoveObject",
				"UploadFile",
				"UploadFileInParts",
				"UploadFileByURL"
			]
		},
		{
			"name": "Manage public access",
			"methods": [
				"PublishObject",
				"CancelObjectPublication",
				"GetPublishedObjectsList",
				"GetPublicObject",
				"GetDownloadLinkForPublicObject",
				"SavePublicObjectToDisk"
			]
		}
	],
	"methods": [
		{
			"name": "GetConfirmationCode",
			"id": "GETCONFIRMATIONCODE",
			"description": "Gets the confirmation code and the address of the page where it needs to be entered",
			"params": [
				{
					"name": "--id",
					"types": "String",
					"description": "Client id",
					"default": null,
					"short": "-i"
				}
			],
			"region": "Authorization"
		},
		{
			"name": "ConvertCodeToToken",
			"id": "CONVERTCODETOTOKEN",
			"description": "Converts the code to a token after entering the code when executing GetConfirmationCode",
			"params": [
				{
					"name": "--id",
					"types": "String",
					"description": "Client id",
					"default": null,
					"short": "-i"
				},
				{
					"name": "--secret",
					"types": "String",
					"description": "Client secret",
					"default": null,
					"short": "-s"
				},
				{
					"name": "--device",
					"types": "String",
					"description": "device_code from GetConfirmationCode",
					"default": null,
					"short": "-d"
				}
			],
			"region": "Authorization"
		},
		{
			"name": "RefreshToken",
			"id": "REFRESHTOKEN",
			"description": "Updates token by Refresh token",
			"params": [
				{
					"name": "--id",
					"types": "String",
					"description": "Client id",
					"default": null,
					"short": "-i"
				},
				{
					"name": "--secret",
					"types": "String",
					"description": "Client secret",
					"default": null,
					"short": "-s"
				},
				{
					"name": "--refresh",
					"types": "String",
					"description": "Refresh token",
					"default": null,
					"short": "-r"
				}
			],
			"region": "Authorization"
		},
		{
			"name": "GetDiskInformation",
			"id": "GETDISKINFORMATION",
			"description": "Gets information about the current disk",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "CreateFolder",
			"id": "CREATEFOLDER",
			"description": "Creates a directory on the disk",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to the created folder",
					"default": null,
					"short": "-p"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "GetObject",
			"id": "GETOBJECT",
			"description": "Gets information about a disk object at the specified path",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to folder or file",
					"default": null,
					"short": "-p"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "DeleteObject",
			"id": "DELETEOBJECT",
			"description": "Deletes an object at the specified path",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to the folder or file to be deleted",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--can",
					"types": "Boolean",
					"description": "To cart",
					"default": true,
					"short": "-c"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "CreateObjectCopy",
			"id": "CREATEOBJECTCOPY",
			"description": "Creates a copy of the object at the specified path and path to the original",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--from",
					"types": "String",
					"description": "Path to the original file or directory",
					"default": null,
					"short": "-f"
				},
				{
					"name": "--to",
					"types": "String",
					"description": "Destination path for the copy",
					"default": null,
					"short": "-o"
				},
				{
					"name": "--rewrite",
					"types": "Boolean",
					"description": "Overwrite if a file with the same name already exists",
					"default": false,
					"short": "-r"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "GetDownloadLink",
			"id": "GETDOWNLOADLINK",
			"description": "Gets a download link for the file",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to the file for downloading",
					"default": null,
					"short": "-p"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "GetFileUploadLink",
			"id": "GETFILEUPLOADLINK",
			"description": "Gets a link for manual file upload to Disk",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path for saving the file to disk",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--rewrite",
					"types": "Boolean",
					"description": "Overwrite if a file with the same name already exists",
					"default": false,
					"short": "-r"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "DownloadFile",
			"id": "DOWNLOADFILE",
			"description": "Downloads a file at the specified path",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to the file for downloading",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--out",
					"types": "String",
					"description": "File save path",
					"default": "Пустое значение",
					"short": "-o"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "GetFilesList",
			"id": "GETFILESLIST",
			"description": "Gets a list of files with or without filtering by type\n    |    List of available types: audio, backup, book, compressed, data, development,\n    |    diskimage, document, encoded, executable, flash, font,\n    |    mage, settings, spreadsheet, text, unknown, video, web",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--amount",
					"types": "Number, String",
					"description": "Number of returned objects",
					"default": "Пустое значение",
					"short": "-a"
				},
				{
					"name": "--offset",
					"types": "Number",
					"description": "Offset for getting objects not from the beginning of the list",
					"default": "Пустое значение",
					"short": "-o"
				},
				{
					"name": "--type",
					"types": "String",
					"description": "Filter by file type",
					"default": "Пустое значение",
					"short": "-p"
				},
				{
					"name": "--datesort",
					"types": "Boolean",
					"description": "True > sort by date, False > alphabetically",
					"default": false,
					"short": "-d"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "MoveObject",
			"id": "MOVEOBJECT",
			"description": "Moves the object to the specified path and path to the original",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--from",
					"types": "String",
					"description": "Path to the original file or folder",
					"default": null,
					"short": "-f"
				},
				{
					"name": "--to",
					"types": "String",
					"description": "Destination path for moving",
					"default": null,
					"short": "-o"
				},
				{
					"name": "--rewrite",
					"types": "Boolean",
					"description": "Overwrite if a file with the same name already exists",
					"default": false,
					"short": "-r"
				}
			],
			"region": "File and folder management"
		},
		{
			"name": "UploadFile",
			"id": "UPLOADFILE",
			"description": "Uploads a file to disk at the specified path",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path for saving the file to disk",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--file",
					"types": "String, BinaryData",
					"description": "File for upload",
					"default": null,
					"short": "-f"
				},
				{
					"name": "--rewrite",
					"types": "Boolean",
					"description": "Overwrite if a file with the same name already exists",
					"default": false,
					"short": "-r"
				}
			],
			"region": "File and folder management"
		},
