			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to place the downloaded file",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--url",
					"types": "String",
					"description": "File URL",
					"default": null,
					"short": "-u"
				}
			],
			"region": "File and folder management"
		},
