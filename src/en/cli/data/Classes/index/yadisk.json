				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to the object to be published",
					"default": null,
					"short": "-p"
				}
			],
			"region": "Manage public access"
		},
