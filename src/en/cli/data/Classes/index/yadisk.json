			"name": "UploadFileInParts",
			"id": "UPLOADFILEINPARTS",
			"description": "Uploads a file to disk at the specified path in parts",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path for saving the file to disk",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--file",
					"types": "String, BinaryData",
					"description": "File for upload",
					"default": null,
					"short": "-f"
				},
				{
					"name": "--psize",
					"types": "Number",
					"description": "Part size when uploading",
					"default": 33554432,
					"short": "-s"
				},
				{
					"name": "--rewrite",
					"types": "Boolean",
					"description": "Overwrite if a file with the same name already exists",
					"default": false,
					"short": "-r"
				}
			],
			"region": "File and folder management"
		},
