{
	"module": "OPI_ClickHouse",
	"library": "clickhouse",
	"self_depend": false,
	"dependencies": [
		"OPI_GRPC"
	],
	"regions": [
		{
			"name": "Common methods",
			"methods": [
				"ExecuteRequest",
				"GetExternalTableStructure",
				"GetHTTPConnectionSettings",
				"GetRequestSettings",
				"GetSessionSettings"
			]
		},
		{
			"name": "Grpc",
			"methods": [
				"GetGRPCConnectionSettings",
				"GetTlsSettings",
				"ProcessGRPCReceiving",
				"ProcessGRPCSending"
			]
		}
	],
	"methods": [
		{
			"name": "ExecuteRequest",
			"id": "EXECUTEREQUEST",
			"description": "Executes a request with the specified parameters",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Connection settings or object (for gRPC)",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--req",
					"types": "Structure Of KeyAndValue",
					"description": "Request data. See GetRequestSettings",
					"default": null,
					"short": "-r"
				},
				{
					"name": "--session",
					"types": "Structure Of KeyAndValue",
					"description": "Session settings. See GetSessionSettings",
					"default": "Empty value",
					"short": "-s"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "GetHTTPConnectionSettings",
			"id": "GETHTTPCONNECTIONSETTINGS",
			"description": "Gets the settings structure for HTTP connection",
			"params": [
				{
					"name": "--url",
					"types": "String",
					"description": "Connection address with protocol and port",
					"default": null,
					"short": "-u"
				},
				{
					"name": "--auth",
					"types": "String, Structure Of KeyAndValue",
					"description": "Authorization: string for JWT, structure for basic",
					"default": "Empty value",
					"short": "-a"
				},
				{
					"name": "--headers",
					"types": "Map Of KeyAndValue",
					"description": "Additional request headers",
					"default": "Empty value",
					"short": "-h"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "GetRequestSettings",
			"id": "GETREQUESTSETTINGS",
			"description": "Forms the request description structure",
			"params": [
				{
					"name": "--query",
					"types": "String",
					"description": "Request text",
					"default": null,
					"short": "-q"
				},
				{
					"name": "--db",
					"types": "String",
					"description": "Database",
					"default": "Empty value",
					"short": "-d"
				},
				{
					"name": "--id",
					"types": "String",
					"description": "Unique request ID, if necessary",
					"default": "Empty value",
					"short": "-i"
				},
				{
					"name": "--data",
					"types": "Arbitrary",
					"description": "String, file, or binary data of the request",
					"default": "Empty value",
					"short": "-t"
				},
				{
					"name": "--format",
					"types": "String",
					"description": "Response format: JSON, CSV, TSV, etc..",
					"default": "JSON",
					"short": "-f"
				},
				{
					"name": "--ext",
					"types": "Array of Structure",
					"description": "External tables data. See GetExternalTableStructure",
					"default": "Empty value",
					"short": "-e"
				},
				{
					"name": "--settings",
					"types": "Map Of KeyAndValue",
					"description": "Additional query parameters of the request",
					"default": "Empty value",
					"short": "-s"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "GetExternalTableStructure",
			"id": "GETEXTERNALTABLESTRUCTURE",
			"description": "Gets the description structure of the external table of the request",
			"params": [
				{
					"name": "--name",
					"types": "String",
					"description": "Table name",
					"default": null,
					"short": "-n"
				},
				{
					"name": "--cols",
					"types": "Structure Of KeyAndValue",
					"description": "Table column structure: Key > name, Value > data type",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--data",
					"types": "Arbitrary",
					"description": "String, file, or binary data of the table",
					"default": "Empty value",
					"short": "-d"
				},
				{
					"name": "--format",
					"types": "String",
					"description": "Data format: CVS, TVS, JSON, etc..",
					"default": "Empty value",
					"short": "-f"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "GetSessionSettings",
			"id": "GETSESSIONSETTINGS",
			"description": "Forms the session settings structure for executing the request",
			"params": [
				{
					"name": "--id",
					"types": "String",
					"description": "Session ID. A new unique identifier if not specified (a new session will be created)",
					"default": "Empty value",
					"short": "-i"
				},
				{
					"name": "--check",
					"types": "Boolean",
					"description": "Check for session existence. True when ID is specified and false when not, unless otherwise specified",
					"default": "Empty value",
					"short": "-c"
				},
				{
					"name": "--timeout",
					"types": "Number",
					"description": "Session lifetime in seconds",
					"default": 60,
					"short": "-t"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "GetGRPCConnectionSettings",
			"id": "GETGRPCCONNECTIONSETTINGS",
			"description": "Gets the settings structure for gRPC connection",
			"params": [
				{
					"name": "--url",
					"types": "String",
					"description": "Connection address with protocol and port",
					"default": null,
					"short": "-u"
				},
				{
					"name": "--auth",
					"types": "String, Structure Of KeyAndValue",
					"description": "Authorization: string for JWT, structure for basic",
					"default": "Empty value",
					"short": "-a"
				},
				{
					"name": "--meta",
					"types": "Structure Of KeyAndValue",
					"description": "gRPC metadata structure, if necessary",
					"default": "Empty value",
					"short": "-m"
				},
				{
					"name": "--tls",
					"types": "Structure Of KeyAndValue",
					"description": "TLS settings. See GetTlsSettings",
					"default": "Empty value",
					"short": "-t"
				}
			],
			"region": "Grpc"
		},
		{
			"name": "ProcessGRPCSending",
			"id": "PROCESSGRPCSENDING",
			"description": "Initializes the stream, sends the request and a set of data in a specified order, receiving one message upon completion",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection parameters",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--req",
					"types": "Structure Of KeyAndValue",
					"description": "Request data. See GetRequestSettings",
					"default": null,
					"short": "-r"
				},
				{
					"name": "--data",
					"types": "Array Of Arbitrary",
					"description": "An array of strings, collections, binary data, or file paths",
					"default": null,
					"short": "-d"
				},
				{
					"name": "--session",
					"types": "Structure Of KeyAndValue",
					"description": "Session settings. See GetSessionSettings",
					"default": "Empty value",
					"short": "-s"
				},
				{
					"name": "--tout",
					"types": "Number",
					"description": "Timeout (in ms)",
					"default": 10000,
					"short": "-t"
				}
			],
			"region": "Grpc"
		},
		{
			"name": "ProcessGRPCReceiving",
			"id": "PROCESSGRPCRECEIVING",
			"description": "Initializes the stream, sends the request, and receives messages until closure or a set limit is reached",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection parameters",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--req",
					"types": "Structure Of KeyAndValue",
					"description": "Request data. See GetRequestSettings",
					"default": null,
					"short": "-r"
				},
				{
					"name": "--amount",
					"types": "Number",
					"description": "Maximum number of messages to receive, if necessary",
					"default": "Empty value",
					"short": "-a"
				},
				{
					"name": "--session",
					"types": "Structure Of KeyAndValue",
					"description": "Session settings. See GetSessionSettings",
					"default": "Empty value",
					"short": "-s"
				},
				{
					"name": "--tout",
					"types": "Number",
					"description": "Timeout (in ms)",
					"default": 10000,
					"short": "-t"
				}
			],
			"region": "Grpc"
		},
		{
			"name": "GetTlsSettings",
			"id": "GETTLSSETTINGS",
			"description": "Forms settings for using TLS",
			"params": [
				{
					"name": "--trust",
					"types": "Boolean",
					"description": "Allows to work with invalid certificates, including self signed",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--cert",
					"types": "String",
					"description": "Path to the root PEM file of the certificate if it is not in the system repository",
					"default": "Empty value",
					"short": "-c"
				}
			],
			"region": "Grpc"
		}
	]
}