{
	"module": "OPI_SFTP",
	"library": "sftp",
	"self_depend": false,
	"regions": [
		{
			"name": "Common methods",
			"methods": [
				"GetConnectionConfiguration",
				"UpdatePath",
				"GetSettingsLoginPassword",
				"GetSettingsPrivateKey",
				"GetSettingsViaAgent",
				"GetProxySettings"
			]
		},
		{
			"name": "Directory management",
			"methods": [
				"ListObjects",
				"CreateNewDirectory",
				"DeleteDirectory",
				"GetCurrentDirectory"
			]
		},
		{
			"name": "File management",
			"methods": [
				"UploadFile",
				"DeleteFile",
				"SaveFile",
				"GetFileInformation"
			]
		}
	],
	"methods": [
		{
			"name": "GetConnectionConfiguration",
			"id": "GETCONNECTIONCONFIGURATION",
			"description": "Forms a complete structure of connection settings that can be used instead of the actual connection when calling other functions",
			"params": [
				{
					"name": "--set",
					"types": "Structure Of KeyAndValue",
					"description": "SSH settings",
					"default": null,
					"short": "-s"
				},
				{
					"name": "--proxy",
					"types": "Structure Of KeyAndValue",
					"description": "Proxy settings, if required. See GetProxySettings",
					"default": "Пустое значение",
					"short": "-p"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "UpdatePath",
			"id": "UPDATEPATH",
			"description": "Changes the object's path to the specified one",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection configuration",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--old",
					"types": "String",
					"description": "Current path to object",
					"default": null,
					"short": "-o"
				},
				{
					"name": "--new",
					"types": "String",
					"description": "New path to object",
					"default": null,
					"short": "-n"
				},
				{
					"name": "--rw",
					"types": "Boolean",
					"description": "Overwrite if an object already exists at the target path",
					"default": false,
					"short": "-r"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "GetSettingsLoginPassword",
			"id": "GETSETTINGSLOGINPASSWORD",
			"description": "password)\n    |    Gets connection settings with login and password authentication",
			"params": [
				{
					"name": "--host",
					"types": "String",
					"description": "SSH host",
					"default": null,
					"short": "-h"
				},
				{
					"name": "--port",
					"types": "Number",
					"description": "SSH port",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--user",
					"types": "String",
					"description": "SSH username",
					"default": null,
					"short": "-u"
				},
				{
					"name": "--pass",
					"types": "String",
					"description": "SSH user password",
					"default": "Пустое значение",
					"short": "-s"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "GetSettingsPrivateKey",
			"id": "GETSETTINGSPRIVATEKEY",
			"description": "Gets connection settings with private key authentication",
			"params": [
				{
					"name": "--host",
					"types": "String",
					"description": "SSH host",
					"default": null,
					"short": "-h"
				},
				{
					"name": "--port",
					"types": "Number",
					"description": "SSH port",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--user",
					"types": "String",
					"description": "SSH username",
					"default": null,
					"short": "-u"
				},
				{
					"name": "--key",
					"types": "String",
					"description": "Path to private key file",
					"default": null,
					"short": "-k"
				},
				{
					"name": "--pub",
					"types": "String",
					"description": "Path to public key file",
					"default": "Пустое значение",
					"short": "-b"
				},
				{
					"name": "--pass",
					"types": "String",
					"description": "Private key password (passphrase)",
					"default": "Пустое значение",
					"short": "-s"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "GetSettingsViaAgent",
			"id": "GETSETTINGSVIAAGENT",
			"description": "Gets connection settings with SSH Agent authentication",
			"params": [
				{
					"name": "--host",
					"types": "String",
					"description": "SSH host",
					"default": null,
					"short": "-h"
				},
				{
					"name": "--port",
					"types": "Number",
					"description": "SSH port",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--user",
					"types": "String",
					"description": "SSH username",
					"default": null,
					"short": "-u"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "GetProxySettings",
			"id": "GETPROXYSETTINGS",
			"description": "Creates a structure of proxy server settings for the connection",
			"params": [
				{
					"name": "--addr",
					"types": "String",
					"description": "Proxy address",
					"default": null,
					"short": "-a"
				},
				{
					"name": "--port",
					"types": "Number",
					"description": "Proxy port",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--type",
					"types": "String",
					"description": "Proxy type: socks5, socks4, http",
					"default": "socks5",
					"short": "-t"
				},
				{
					"name": "--login",
					"types": "String, Undefined",
					"description": "Authorization login, if required",
					"default": "Пустое значение",
					"short": "-l"
				},
				{
					"name": "--pass",
					"types": "String, Undefined",
					"description": "Authorization password, if required",
					"default": "Пустое значение",
					"short": "-s"
				}
			],
			"region": "Common methods"
		},
		{
			"name": "ListObjects",
			"id": "LISTOBJECTS",
			"description": "Gets information about the contents of a directory at the specified path",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection configuration",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to search directory",
					"default": "Пустое значение",
					"short": "-p"
				},
				{
					"name": "--rcv",
					"types": "Boolean",
					"description": "Get information about objects in nested directories",
					"default": false,
					"short": "-r"
				}
			],
			"region": "Directory management"
		},
		{
			"name": "CreateNewDirectory",
			"id": "CREATENEWDIRECTORY",
			"description": "Creates a directory at the specified path",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection configuration",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to new directory",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--mode",
					"types": "String",
					"description": "Access mode to the directory in POSIX numeric format (as in chmod)",
					"default": 700,
					"short": "-m"
				}
			],
			"region": "Directory management"
		},
		{
			"name": "DeleteDirectory",
			"id": "DELETEDIRECTORY",
			"description": "Deletes an existing directory",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection configuration",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to directory to delete",
					"default": null,
					"short": "-p"
				}
			],
			"region": "Directory management"
		},
		{
			"name": "GetCurrentDirectory",
			"id": "GETCURRENTDIRECTORY",
			"description": "Gets the current directory from which relative paths are calculated",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection configuration",
					"default": null,
					"short": "-c"
				}
			],
			"region": "Directory management"
		},
		{
			"name": "UploadFile",
			"id": "UPLOADFILE",
			"description": "Uploads a file from disk or binary data to the server",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection configuration",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--file",
					"types": "String, BinaryData",
					"description": "File on disk or file data",
					"default": null,
					"short": "-f"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to save file on server",
					"default": null,
					"short": "-p"
				}
			],
			"region": "File management"
		},
		{
			"name": "DeleteFile",
			"id": "DELETEFILE",
			"description": "Delete file from server",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection configuration",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to file on server",
					"default": null,
					"short": "-p"
				}
			],
			"region": "File management"
		},
		{
			"name": "SaveFile",
			"id": "SAVEFILE",
			"description": "Saves the file from the server to the specified path",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection configuration",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to file on server",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--file",
					"types": "String",
					"description": "Path to save file on disk",
					"default": null,
					"short": "-f"
				}
			],
			"region": "File management"
		},
		{
			"name": "GetFileInformation",
			"id": "GETFILEINFORMATION",
			"description": "Get file information by the specified path",
			"params": [
				{
					"name": "--conn",
					"types": "Arbitrary",
					"description": "Existing connection or connection configuration",
					"default": null,
					"short": "-c"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to file on server",
					"default": null,
					"short": "-p"
				}
			],
			"region": "File management"
		}
	]
}