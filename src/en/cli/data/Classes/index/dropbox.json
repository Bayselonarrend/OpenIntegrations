				{
					"name": "--refresh",
					"types": "String",
					"description": "Refresh token",
					"default": "",
					"short": "-r"
				}
			],
			"region": "Account and authorization"
		},
		{
			"name": "GetAccountInformation",
			"id": "GETACCOUNTINFORMATION",
			"description": "Gets account information",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": "",
					"short": "-t"
