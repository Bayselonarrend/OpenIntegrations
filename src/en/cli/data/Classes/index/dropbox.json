				{
					"name": "--account",
					"types": "String",
					"description": "Account ID. Current token account if not filled",
					"default": "Пустое значение",
					"short": "-a"
				}
			],
			"region": "Account and authorization"
		},
		{
			"name": "GetSpaceUsageData",
			"id": "GETSPACEUSAGEDATA",
			"description": "Gets information on the amount of used disk space",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": "",
					"short": "-t"
				}
			],
			"region": "Account and authorization"
		},
		{
			"name": "GetObjectInformation",
			"id": "GETOBJECTINFORMATION",
			"description": "Gets information about a file or directory",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": "",
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to the object",
					"default": "",
					"short": "-p"
