			"description": "Gets the list of the first files in the directory or continues getting the next ones when the cursor is specified",
			"params": [
				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": "",
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to the directory. Optional if the cursor is specified",
					"default": "Пустое значение",
					"short": "-p"
				},
				{
					"name": "--detail",
					"types": "Boolean",
					"description": "Add information fields for mediafiles",
					"default": false,
					"short": "-d"
				},
				{
					"name": "--cursor",
					"types": "String",
					"description": "Cursor from the previous request to get the next set of files",
					"default": "Пустое значение",
					"short": "-c"
				}
			],
			"region": "File and directory management"
		},
