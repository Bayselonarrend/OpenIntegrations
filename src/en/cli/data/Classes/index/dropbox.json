				{
					"name": "--token",
					"types": "String",
					"description": "Token",
					"default": "",
					"short": "-t"
				},
				{
					"name": "--path",
					"types": "String",
					"description": "Path to the object",
					"default": "",
					"short": "-p"
				}
			],
			"region": "File and directory management"
		},
