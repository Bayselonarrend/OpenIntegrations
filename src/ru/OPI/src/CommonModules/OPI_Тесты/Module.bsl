    Обработать(Результат, "FTP", "СоздатьНовуюДиректорию", "Двойная, " + Постфикс);

    OPI_Инструменты.Пауза(5);

    Проверка = OPI_FTP.ПолучитьСписокОбъектов(Соединение, "new_dir", Истина);

