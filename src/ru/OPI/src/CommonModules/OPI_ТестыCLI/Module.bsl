    Если Не Курсор["result"] Тогда
        ВызватьИсключение Курсор["error"];
    КонецЕсли;

    Курсор            = Курсор["data"]["cursor"];
    МассивДокументов  = Курсор["firstBatch"];
    IDКурсора         = Курсор["id"];
