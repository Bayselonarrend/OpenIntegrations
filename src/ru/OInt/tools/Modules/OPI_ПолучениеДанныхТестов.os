    Если Не ЗначениеЗаполнено(Вариант) Тогда

        ОжидаетЧто(Результат["result"]).Равно(Истина);
        ОжидаетЧто(Результат["data"].Количество() > 0).Равно(Истина);
        ОжидаетЧто(Результат["data"][0]["stringField"]).Равно("Текст");

    ИначеЕсли Вариант = 1 Тогда
        // Фильтрация по категории и диапазону цен
        ОжидаетЧто(Результат["result"]).Равно(Истина);
        ОжидаетЧто(Результат["data"].Количество() > 0).Равно(Истина);

        Для Каждого Документ Из Результат["data"] Цикл
            ОжидаетЧто(Документ["category"]).Равно("электроника");
            ОжидаетЧто(Документ["price"] >= 100).Равно(Истина);
            ОжидаетЧто(Документ["price"] <= 400).Равно(Истина);
        КонецЦикла;

        Если Результат["data"].Количество() > 1 Тогда
            ОжидаетЧто(Результат["data"][0]["price"] <= Результат["data"][1]["price"]).Равно(Истина);
        КонецЕсли;

    ИначеЕсли Вариант = 2 Тогда
        ОжидаетЧто(Результат["result"]).Равно(Истина);

        Для Каждого Документ Из Результат["data"] Цикл
            ОжидаетЧто(Документ["inStock"]).Равно(Истина);
            ОжидаетЧто(Документ["rating"] >= 4).Равно(Истина);
        КонецЦикла;

        Если Результат["data"].Количество() > 1 Тогда
            ОжидаетЧто(Результат["data"][0]["rating"] >= Результат["data"][1]["rating"]).Равно(Истина);
        КонецЕсли;

    ИначеЕсли Вариант = 3 Тогда
        ОжидаетЧто(Результат["result"]).Равно(Истина);

        Для Каждого Документ Из Результат["data"] Цикл

            Найден         = Ложь;
            Для Каждого Тэг Из Документ["tags"] Цикл
                Если СтрНайти(Тэг, "распродажа") > 0 Тогда
                    Найден = Истина;
                    Прервать;
                КонецЕсли;
            КонецЦикла;

            ОжидаетЧто(Найден).Равно(Истина);

        КонецЦикла;

    ИначеЕсли Вариант = 4 Тогда

        ОжидаетЧто(Результат["result"]).Равно(Истина);

        Для Каждого Документ Из Результат["data"] Цикл
            ОжидаетЧто(Документ["details"]["supplier"]).Равно("Поставщик А");
            ОжидаетЧто(Документ["details"]["weightKg"] < 3).Равно(Истина);
        КонецЦикла;

    ИначеЕсли Вариант = 5 Тогда

        ОжидаетЧто(Результат["result"]).Равно(Истина);

        Для Каждого Документ Из Результат["data"] Цикл
            ОжидаетЧто(Документ["productName"] = Неопределено).Равно(Ложь);
            ОжидаетЧто(Документ["price"]       = Неопределено).Равно(Ложь);
            ОжидаетЧто(Документ["rating"]      = Неопределено).Равно(Ложь);
            ОжидаетЧто(Документ["category"]).Равно(Неопределено);
            ОжидаетЧто(Документ["inStock"]).Равно(Неопределено);
            ОжидаетЧто(Документ["details"]).Равно(Неопределено);
        КонецЦикла;

    КонецЕсли;

    Возврат Результат;

