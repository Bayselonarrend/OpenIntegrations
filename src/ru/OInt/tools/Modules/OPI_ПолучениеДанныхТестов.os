Функция Проверка_YandexDisk_ЗагрузитьФайлЧастями(Знач Результат, Знач Вариант, Файл = "")

    Если Вариант = "Скачивание" Тогда

        OPI_ПреобразованиеТипов.ПолучитьДвоичныеДанные(Файл);

        ОжидаетЧто(Результат).ИмеетТип("ДвоичныеДанные").Заполнено();
        ОжидаетЧто(Результат.Размер()).Равно(Файл.Размер());

        ХешОригинал = ПолучитьСтрокуИзДвоичныхДанных(OPI_Криптография.Хеш(Файл, ХешФункция.SHA256));
        ХешПроверки = ПолучитьСтрокуИзДвоичныхДанных(OPI_Криптография.Хеш(Результат, ХешФункция.SHA256));

        ОжидаетЧто(ХешПроверки).Равно(ХешОригинал);

    ИначеЕсли Вариант = "Удаление" Тогда

        Если Не нРег(Строка(Результат)) = "null" Тогда
            ОжидаетЧто(ЗначениеЗаполнено(Результат)).Равно(Ложь);
        КонецЕсли;

    Иначе
        ОжидаетЧто(Результат["status"] >= 200 И Результат["status"] < 300).Равно(Истина);
    КонецЕсли;

    OPI_Инструменты.Пауза(2);

    Возврат Результат;

