{
	"module": "OPI_GoogleWorkspace",
	"library": "google",
	"self_depend": false,
	"dependencies": null,
	"regions": [
		{
			"name": "Программный интерфейс",
			"methods": [
				"СформироватьСсылкуПолученияКода",
				"ПолучитьТокенПоКоду",
				"ОбновитьТокен",
				"ПолучитьТокенServiceАккаунта"
			]
		}
	],
	"methods": [
		{
			"name": "СформироватьСсылкуПолученияКода",
