{
	"module": "OPI_TCP",
	"library": "tcp",
	"self_depend": false,
	"dependencies": null,
	"regions": [],
	"methods": [
		{
			"name": "ОбработатьЗапрос",
			"id": "ОБРАБОТАТЬЗАПРОС",
			"description": "Отправляет одиночный запрос на указанный адрес и получает ответ, используя стандартные настройки",
			"params": [
				{
					"name": "--address",
					"types": "Строка",
					"description": "Адрес и порт для подключения",
					"default": null,
					"short": "-a"
				},
				{
					"name": "--data",
					"types": "Строка, ДвоичныеДанные",
					"description": "Данные или текст для отправки",
					"default": "Пустое значение",
					"short": "-d"
				},
				{
					"name": "--string",
					"types": "Булево",
					"description": "Признак получения ответа как строки",
					"default": "true",
					"short": "-s"
				},
				{
					"name": "--tls",
					"types": "Структура Из КлючИЗначение",
					"description": "Настройки TLS, если необходимо. См. ПолучитьНастройкиTls",
					"default": "Пустое значение",
					"short": "-t"
				},
				{
					"name": "--proxy",
					"types": "Структура Из КлючИЗначение",
					"description": "Настройки прокси, если необходимо. См. ПолучитьНастройкиПрокси",
					"default": "Пустое значение",
					"short": "-p"
				}
			],
			"region": "Методы клиента"
		},
		{
			"name": "ПолучитьНастройкиПрокси",
			"id": "ПОЛУЧИТЬНАСТРОЙКИПРОКСИ",
			"description": "Формирует структуру настроек прокси-сервера для соединения",
			"params": [
				{
					"name": "--addr",
					"types": "Строка",
					"description": "Адрес прокси",
					"default": null,
					"short": "-a"
				},
				{
					"name": "--port",
					"types": "Число",
					"description": "Порт прокси",
					"default": null,
					"short": "-p"
				},
				{
					"name": "--type",
					"types": "Строка",
					"description": "Вид прокси: socks5, socks4, http",
					"default": "socks5",
					"short": "-t"
				},
				{
					"name": "--login",
					"types": "Строка, Неопределено",
					"description": "Логин авторизации, если необходимо",
					"default": "Пустое значение",
					"short": "-l"
				},
				{
					"name": "--pass",
					"types": "Строка, Неопределено",
					"description": "Пароль для авторизации, если необходимо",
					"default": "Пустое значение",
					"short": "-s"
				}
			],
			"region": "Методы клиента"
		},
		{
			"name": "ПолучитьНастройкиTls",
			"id": "ПОЛУЧИТЬНАСТРОЙКИTLS",
			"description": "Формирует настройки для использования TLS при выполнении запросов",
			"params": [
				{
					"name": "--trust",
					"types": "Булево",
					"description": "Позволяет работать с некорректными сертификатами, в т.ч. самоподписанными",
					"default": null,
					"short": "-t"
				},
				{
					"name": "--cert",
					"types": "Строка",
					"description": "Путь к корневому PEM файлу сертификата, если его нет в системном хранилище",
					"default": "Пустое значение",
					"short": "-c"
				}
			],
			"region": "Методы клиента"
		}
	]
}